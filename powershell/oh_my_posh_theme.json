{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "@{{ .UserName }} âžœ",
          "foreground": "#FFE082",
          "type": "session",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "style": "agnoster_short"
          },
          "template": " {{ .Path }} ",
          "foreground": "#56B6C2",
          "type": "path",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "(\ue626{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}) ",
          "foreground": "#7FD5EA",
          "type": "go",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "(\ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }}) ",
          "foreground": "#9e7eff",
          "type": "python",
          "style": "powerline"
        },
        {
          "properties": {
            "branch_icon": "",
            "cache_duration": "none"
          },
          "template": "<#E8CC97>git(</>{{ .HEAD }}<#E8CC97>) </>",
          "foreground": "#56B6C2",
          "type": "git",
          "style": "plain"
        },
        {
          "properties": {
            "always_enabled": false,
            "cache_duration": "none",
            "style": "austin",
            "threshold": 100
          },
          "template": "{{ .FormattedMs }}",
          "foreground": "#FFAB91",
          "type": "executiontime",
          "style": "powerline"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": ">",
          "foreground": "#FFFFFF",
          "type": "text",
          "style": "plain"
        }
      ],
      "newline": true
    }
  ],
  "version": 3,
  "final_space": true
}
